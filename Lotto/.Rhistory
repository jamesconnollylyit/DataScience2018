combine.results <- function(file_list, file_location) {     #Initialise all_lotto_data variable     #Note that it hasn't been assigned to a specific type yet     all_lotto_data <- NULL     for (csv_file in file_list) {         #Read each of the csv files in turn and skip the first line as it         #contains a header line         lotto_data <- read.csv(header = TRUE, paste("c:/lotto/", csv_file, sep = ""), stringsAsFactors = FALSE)         #Only select attributes we're interested in         #We don't need the first attribute         data_of_interest <- lotto_data[1:8]         # append to the allCrimeData data frame         all_lotto_data <- rbind(all_lotto_data, data_of_interest)     }     return(all_lotto_data) }
